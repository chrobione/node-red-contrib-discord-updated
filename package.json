  "main": "discord/discord.js",
  "scripts": {
    "test": "mocha \"test/**/*_spec.js\""
  },
  "author": "Mark Koudstaal",
  "contributors": [
    {
      "name": "Javier Colombera",
      "email": "jcolombera@gmail.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "discord.js": "^14.15.3",
    "flatted": "^3.2.9"
  },
  "bugs": {
    "url": "https://github.com/chrobione/node-red-contrib-discord-updated/issues"
